number between -10 and 19

for(var i=-11; i<19; i+1){
console.log(i);
}

even numbers between 10 and 40
for(var i=10; i<40; i+2){
console.log(i);
}

odd numbers between 300 and 333
for(var i=301; i<334; i+2){
console.log(i);
}

all numbers divis by 5 and 3
for(var i=5; i<50; (i%5&&i%3)=0; i+1){
console.log(i);
}
